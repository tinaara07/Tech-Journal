<div class="profile">
    <h1>Your Profile</h1>
    <nav>
        <a href="/">Home</a>

    </nav>
    <div class="journal-list">
        <h2>Your Journal</h2>
        {{#each journals}}
            <div class="journal">
                <h3>{{title}}</h3>
                <p>Posted on {{dateCreated}}</p>
                <a href="/editjournal/{{id}}">Edit</a>
                <button class="delete-journal" data-id="{{id}}">Delete</button>
            </div>
        {{else}}
            <p>You have no posts yet.</p>
        {{/each}}
        <button id="new-journal">Add New Journal</button>
    </div>
</div>
 <script src="/js/profile.js"></script>